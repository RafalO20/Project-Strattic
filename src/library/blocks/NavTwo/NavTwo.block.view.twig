<div class="m-HeaderPrimary m-HeaderPrimaryScroll">
  <div class="m-HeaderPrimary__container">
    <div class="m-HeaderPrimary__container-left">
      <a href="http://localhost/Strattic/">
        <img class="m-HeaderPrimary__logo" src="{{theme.link}}/static/img/logoMain.svg" alt="logo" />
      </a>
      <div class="m-HeaderPrimary__rectangle"></div>
      <ul class="m-HeaderPrimary__menu -desktop">
      {% for item in nav.primary.items %}
          <li class="m-HeaderPrimary__item">
              {% if item.children %}
                  {% include "modules/Button/Button.module.twig" with {
                      type: "link",
                      color: "black",
                      link: item.link,
                      text: item.title,
                      drop: true
                  }%}
                  <ul class="m-HeaderPrimary__drop">
                      {% for child in item.children %}
                          <li class="m-HeaderPrimary__childItem">
                              {% include "modules/Button/Button.module.twig" with {
                                  type: "link",
                                  color: "black",
                                  link: child.link,
                                  text: child.title
                              }%}
                          </li>
                      {% endfor %}
                  </ul>
              {% else %}
                  {% include "modules/Button/Button.module.twig" with {
                      type: "link",
                      color: "black",
                      link: item.link,
                      text: item.title
                  }%}
              {% endif %}
          </li>
      {% endfor %}
    </ul>
    </div>
    <div class="m-HeaderPrimary__container-right">
      <a class="m-HeaderPrimary__link m-Button-two-login" href="#">Login</a>
      <a class="m-HeaderPrimary__link--red" href="#">Get started</a>
      <div class="m-HeaderPrimary__menu--mobile">
        {% include "modules/BurgerMenu/BurgerMenu.module.twig" with {
          stickColor: "black"
        }%}
      </div>
    </div>
  </div>
</div>
